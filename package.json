{
  "name": "zfg",
  "version": "2.1.7",
  "description": "g2 react packaging version",
  "scripts": {
    "start": "rc-tools run server",
    "build": "rc-tools run build",
    "lint": "rc-tools run lint",
    "compile": "rc-tools run compile --babel-runtime",
    "test": "jest --no-cache --updateSnapshot",
    "pub": "npm run test && npm run build && npm run compile && zf-tools run pub",
    "prepublish": "zf-tools run guard",
    "coverage": "jest --coverage "
  },
  "keywords": [
    "react chart",
    "g2 react chart"
  ],
  "author": [
    "feasin",
    "myzykj",
    "alex-wz"
  ],
  "config": {
    "port": 8018,
    "entry": {
      "cr-comp": [
        "./assets/index.less",
        "./src/index.js"
      ]
    }
  },
  "files": [
    "dist",
    "es",
    "lib",
    "assets/*.css"
  ],
  "main": "./lib/index",
  "module": "./es/index",
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*",
      "!src/utils/**/*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "\\.jsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js"
    }
  },
  "devDependencies": {
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "jest": "^21.2.1",
    "pre-commit": "1.x",
    "rc-tools": "^7.0.2",
    "zf-tools": "0.0.9"
  },
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^4.0.10",
    "@antv/l7": "^2.2.22",
    "@antv/l7-district": "^2.2.22",
    "@antv/l7-maps": "^2.2.22",
    "babel-runtime": "6.x",
    "classnames": "2.x",
    "insert-css": "^2.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.5.6",
    "rc-util": "^4.1.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "license": "MIT"
}
